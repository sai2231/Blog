<div id="post_content">

 <h1> <p class="title">
  <%= @post.title %>
  </p> 
 </h1>

 <p class="date">
	Submitted <%= time_ago_in_words(@post.created_at) %> Ago
	  <%= link_to 'Edit', edit_post_path(@post) %>
	  <%= link_to  'Delete', post_path(@post) %>
 </p>

 <p class="body">
  <%=@post.body %>
 </p>
</div>